{
    "_version": "1.12.0",
    "sap.app": {
        "id": "demo.app.cooking",
        "type": "application"
    },
    "sap.ui5": {
        "dependencies": {
            "minUI5Version": "1.60.0",
            "libs": {
                "sap.ui.core": {},
                "sap.m": {},
				"sap.ui.layout": {}
            }
        },
        "rootView": {
            "viewName": "demo.app.cooking.view.App",
            "type": "JS",
            "async": true,
            "id": "app"
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "demo.app.cooking.i18n.i18n"
                }
            },
            "": {
                "type": "sap.ui.model.json.JSONModel",
                "uri": "model/sample.json"
            }
        },
        "resources": {
            "css": [{
                "uri": "css/styles.css"
            }]
        }
    }
}
